<?xml version="1.0"?>
<transfers
    xmlns="spryker:databuilder-01"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="spryker:databuilder-01 http://static.spryker.com/databuilder-01.xsd"
>

    <transfer name="Secret">
        <property name="value" dataBuilderRule="unique()->bothify('#####?????')"/>
    </transfer>

    <transfer name="SecretKey">
        <property name="prefix" dataBuilderRule="word()"/>
        <property name="identifier" dataBuilderRule="unique()->bothify('#####?????')"/>
    </transfer>

    <transfer name="SecretTag">
        <property name="key" dataBuilderRule="unique()->bothify('#####?????')"/>
        <property name="value" dataBuilderRule="word()"/>
    </transfer>

</transfers>
